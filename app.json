{
  "expo": {
    "name": "expo-react-native-w-tailwind",
    "slug": "expo-react-native-w-tailwind",
    "extra": {
      "redirectUri": "https://auth.expo.io/@roeina/expo-react-native-w-tailwind",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "649d46c5-9122-4865-8465-b99e3d34844f"
      }
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "packege": "com.averagedev.authfirebase",
      "googleServiceFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-google-signin/google-signin"    
    ],
    "owner": "roeina"
  }
}
